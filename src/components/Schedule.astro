---
const eventObject = [
    { startTime: "9:00AM", endTime: "11:00AM", Core: "Check-in", Event: "Team Matching, Career Fair" },
    { startTime: "10:30AM", endTime: "12:00PM", Core: "Walk-in Registration", Event: "" },
    { startTime: "11:00AM", endTime: "11:30AM", Core: "Opening Ceremony", Event: "" },
    { startTime: "12:00PM", endTime: "1:00PM", Core: "Lunch & Hacking Begins", Event: "" },
    { startTime: "1:05PM", endTime: "1:40PM", Core: "", Event: "Intro to Hackathons (B2550)" },
    { startTime: "1:45PM", endTime: "2:20PM", Core: "", Event: "WebLabs Web Dev Workshop (B2550)" },
    { startTime: "2:25PM", endTime: "3:00PM", Core: "", Event: "GWC Software Dev Workshop (B2550)" },
    { startTime: "3:00PM", endTime: "4:00PM", Core: "", Event: "TDS Workshop (B2550)" },
    { startTime: "4:00PM", endTime: "5:00PM", Core: "", Event: "CapitalOne Competition (B2550)" },
    { startTime: "5:00PM", endTime: "6:00PM", Core: "", Event: "" },
    { startTime: "6:00PM", endTime: "7:00PM", Core: "Dinner", Event: "" },
    { startTime: "9:30PM", endTime: "10:45PM", Core: "", Event: "HackY2K (BB2550) & Boba Social (B2556)" },
    { startTime: "11:30PM", endTime: "12:00AM", Core: "", Event: "Estimathon (B2556)" },
    { startTime: "12:00AM", endTime: "6:00AM", Core: "", Event: "Overnight Hacking" },
    { startTime: "8:00AM", endTime: "10:00AM", Core: "Breakfast", Event: "" },
    { startTime: "11:00AM", endTime: "11:30AM", Core: "", Event: "GDC How to Present Workshop (B2550)" },
    { startTime: "12:00PM", endTime: "1:00PM", Core: "Hacking Ends, Lunch", Event: "" },
    { startTime: "1:00PM", endTime: "2:00PM", Core: "Project Judging", Event: "" },
    { startTime: "2:30PM", endTime: "3:15PM", Core: "Closing Ceremony", Event: "Finalist Presentations" },
    { startTime: "3:15PM", endTime: "4:00PM", Core: "Awards Ceremony", Event: "" },
];
---

<div class="w-full mx-auto px-6 md:px-12 xl:px-6 bg-gray-800">
    <div class="relative pb-10 pt-10 sm:pt-10 sm:pb-10 ml-auto min-h-full flex flex-col items-center">
        <h1 id="schedule" class="font-header text-white font-semibold text-3xl md:text-4xl xl:text-5xl mt-12 mb-4">Fall 2025 Schedule</h1>
        <div class="w-lg max-w-full overflow-x-auto">
            <table class="w-full text-sm text-left text-gray-500 dark:text-gray-400 overflow-x-auto">
                <thead class="text-base uppercase bg-gray-700 text-gray-100">
                    <tr>
                        <th scope="col" class="px-6 py-3">Time</th>
                        <th scope="col" class="px-6 py-3">Core</th>
                        <th scope="col" class="px-6 py-3">Event</th>
                    </tr>
                </thead>
                <tbody>
                    {
                    eventObject.map((element) => (
                    <tr class="bg-gray-800">
                        <td class="px-6 py-4 font-medium text-lg whitespace-nowrap text-white">
                            {element.startTime} - {element.endTime}
                        </td>
                        <td class="px-6 py-4 font-medium text-lg whitespace-nowrap text-white">{element.Core}</td>
                        <td class="px-6 py-4 font-medium text-lg whitespace-nowrap text-white">{element.Event}</td>
                    </tr>
                    ))
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
